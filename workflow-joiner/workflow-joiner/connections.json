{
  "managedApiConnections": {
    "azureautomation": {
      "api": {
        "id": "/subscriptions/@appsettings('WORKFLOWS_SUBSCRIPTION_ID')/providers/Microsoft.Web/connections/azureautomation"
      },
      "connection": {
        "id": "/subscriptions/@appsettings('WORKFLOWS_SUBSCRIPTION_ID')/resourceGroups/@appsettings('WORKFLOWS_RESOURCE_GROUP_NAME')/providers/Microsoft.Web/connections/@appsettings('AZUREAUTOMATION_CONNECTION_NAME')"
      },
      "authentication": {
        "type": "ManagedServiceIdentity",
        "identity": {
          "userAssignedIdentity": "@appsettings('USER_ASSIGNED_IDENTITY_RESOURCE_ID')"
        }
      },
      "connectionRuntimeUrl": "@appsettings('AZUREAUTOMATION_CONNECTION_RUNTIME_URL')",
      "connectionProperties": {
        "authentication": {
          "type": "ManagedServiceIdentity",
          "audience": "https://management.core.windows.net/"
        }
      }
    },
    "keyvault": {
      "api": {
        "id": "/subscriptions/@appsettings('WORKFLOWS_SUBSCRIPTION_ID')/providers/Microsoft.Web/locations/switzerlandnorth/managedApis/keyvault"
      },
      "connection": {
        "id": "/subscriptions/@appsettings('WORKFLOWS_SUBSCRIPTION_ID')/resourceGroups/@appsettings('WORKFLOWS_RESOURCE_GROUP_NAME')/providers/Microsoft.Web/connections/@appsettings('KEYVAULT_CONNECTION_NAME')"
      },
      "authentication": {
        "type": "ManagedServiceIdentity",
        "identity": {
          "userAssignedIdentity": "@appsettings('USER_ASSIGNED_IDENTITY_RESOURCE_ID')"
        }
      },
      "connectionRuntimeUrl": "@appsettings('KEYVAULT_CONNECTION_RUNTIME_URL')",
      "connectionProperties": {
        "authentication": {
          "type": "ManagedServiceIdentity",
          "audience": "https://vault.azure.net"
        }
      }
    },
    "acsemail": {
      "api": {
        "id": "/subscriptions/@appsettings('WORKFLOWS_SUBSCRIPTION_ID')/providers/Microsoft.Web/locations/switzerlandnorth/managedApis/acsemail"
      },
      "connection": {
        "id": "/subscriptions/@appsettings('WORKFLOWS_SUBSCRIPTION_ID')/resourceGroups/@appsettings('WORKFLOWS_RESOURCE_GROUP_NAME')/providers/Microsoft.Web/connections/@appsettings('ACSEMAIL_CONNECTION_NAME')"
      },
      "authentication": {
        "type": "ManagedServiceIdentity",
        "identity": {
          "userAssignedIdentity": "@appsettings('USER_ASSIGNED_IDENTITY_RESOURCE_ID')"
        }
      },
      "connectionRuntimeUrl": "@appsettings('ACSEMAIL_CONNECTION_RUNTIME_URL')"
    },
    "azurecommunicationservicessms": {
      "api": {
        "id": "/subscriptions/@appsettings('WORKFLOWS_SUBSCRIPTION_ID')/providers/Microsoft.Web/locations/switzerlandnorth/managedApis/azurecommunicationservicessms"
      },
      "connection": {
        "id": "/subscriptions/@appsettings('WORKFLOWS_SUBSCRIPTION_ID')/resourceGroups/@appsettings('WORKFLOWS_RESOURCE_GROUP_NAME')/providers/Microsoft.Web/connections/@appsettings('AZURECOMMUNICATIONSERVICESSMS_CONNECTION_NAME')"
      },
      "authentication": {
        "type": "ManagedServiceIdentity",
        "identity": {
          "userAssignedIdentity": "@appsettings('USER_ASSIGNED_IDENTITY_RESOURCE_ID')"
        }
      },
      "connectionRuntimeUrl": "@appsettings('AZURECOMMUNICATIONSERVICESSMS_CONNECTION_RUNTIME_URL')"
    }
  },
  "serviceProviderConnections": {
    "AzureFile": {
      "parameterValues": {
        "connectionString": "@appsetting('AzureFile_connectionString')"
      },
      "parameterSetName": "connectionString",
      "serviceProvider": {
        "id": "/serviceProviders/AzureFile"
      },
      "displayName": "@appsettings('AZUREFILE_CONNECTION_NAME')"
    }
  }
}